{
  "version": 2,
  "outputDirectory": "dist",
  "rootDirectory": ".",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
